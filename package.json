{
  "name": "lspu-medicine-inventory-system",
  "version": "1.0.0",
  "description": "Medical inventory system for LSPU university clinic",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js --clean-cache",
    "mac-deps": "npm install sqlite3@5.1.6 --build-from-source --no-audit --no-fund && npm install bcrypt@5.1.1 --build-from-source --no-audit --no-fund",
    "mac-install": "npm install --no-audit --no-fund --loglevel=error",
    "install-mac": "npm run clean && npm run mac-deps && npm run mac-install",
    "install-win": "npm run clean && npm install sqlite3@5.1.6 && npm install bcrypt@5.1.1 && npm install",
    "setup": "node src/scripts/check-platform.js",
    "backup-db": "node src/scripts/backup-database.js backup",
    "restore-db": "node src/scripts/backup-database.js restore",
    "restore-working": "node src/scripts/restore-working-db.js",
    "convert-fonts": "node convert-fonts.js"
  },
  "keywords": [
    "inventory",
    "medical",
    "clinic",
    "medicine"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "blob-stream": "^0.1.3",
    "body-parser": "1.19.0",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "csurf": "1.11.0",
    "dotenv": "16.3.1",
    "ejs": "3.1.9",
    "express": "4.17.1",
    "express-ejs-layouts": "2.5.1",
    "express-session": "1.17.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "3.7.1",
    "multer": "1.4.5-lts.1",
    "mysql": "2.18.1",
    "node-schedule": "2.1.1",
    "pdfkit": "^0.16.0",
    "pdfkit-table": "^0.1.99",
    "puppeteer": "19.11.1",
    "sqlite3": "^5.1.6",
    "vm2": "^3.9.19"
  },
  "devDependencies": {
    "nodemon": "3.0.2",
    "tailwind-scrollbar": "3.1.0"
  },
  "engines": {
    "node": "16.x"
  }
}
